    "keywords": [
        "coveralls", "code coverage", "report"
    ],
    "require-dev": {
        "danielstjules/pho": "1.0.*",
        "jaz303/phake": "0.6.*",
        "phpspec/prophecy": "1.1.*",
        "jasonlewis/resource-watcher": "1.1.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noritaka Horio",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "phpcollection/phpcollection": "0.4.*",
        "guzzle/guzzle": "3.9.*",
        "gitonomy/gitlib": "0.1.*"
    },
    "autoload": {
        "psr-4": { "coverallskit\\": "src/" }
    }
}
